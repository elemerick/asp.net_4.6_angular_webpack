{
  "name": "angular-quickstart",
  "version": "1.0.0",
  "description": "QuickStart for angular on asp.net webapi .net framework 4.6 and webpack",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "./node_modules/.bin/webpack --config webpack.config.js",
    "prebuild:prod": "npm run clean",
    "build:prod": "./node_modules/.bin/webpack --config webpack.prod.config.js",
    "prewp": "npm run clean",
    "wp": "./node_modules/.bin/webpack --config webpack.config.js --watch --watch-aggregate-timeout 2000",
    "prewp:aot": "npm run clean",
    "wp:aot": "./node_modules/.bin/webpack --config webpack.aot.config.js",
    "rimraf": "./node_modules/.bin/rimraf",
    "clean": "npm run rimraf -- ./client/*",
    "reset": "npm cache clean && npm run clean && npm run rimraf -- node_modules",
    "tslint": "tslint --type-check --project tsconfig.json",
    "start": "webpack-dev-server",
    "test": "./node_modules/.bin/karma start karma.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "./node_modules/.bin/protractor ./protractor.conf.js",
    "srcmap": "./node_modules/.bin/source-map-explorer ./client/node-static-2cea67d1c2290838848a.js"
  },
